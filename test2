setfpscap(5)

repeat task.wait() until game:IsLoaded()

local Players = game:GetService("Players")
local lp = Players.LocalPlayer

repeat task.wait() until lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")

task.wait(1)

lp.Character.HumanoidRootPart.CFrame = CFrame.new(9888, 13409, 260)

task.wait()

settings().Rendering.QualityLevel = 1
game:GetService("RunService"):Set3dRenderingEnabled(false)

getgenv().reportTime = 120
getgenv().tryCollectMultipleTimes = true

loadstring(game:HttpGet("https://raw.githubusercontent.com/IdiotHub/Scripts/refs/heads/main/BGSI/AutoClaw.lua"))()
