	local t = Instance.new("TextLabel", p)
	t.Size = UDim2.new(1,0,1,0)
	t.BackgroundTransparency = 1
	t.Font = Enum.Font.GothamBlack
	t.TextScaled = true
	t.TextWrapped = true
	t.TextColor3 = Color3.new(1,1,1)
	return t
end
