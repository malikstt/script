local Players=game:GetService("Players") local lp=Players.LocalPlayer local ReplicatedStorage=game:GetService("ReplicatedStorage") local RunService=game:GetService("RunService") local TweenService=game:GetService("TweenService") local LocalData=require(ReplicatedStorage.Client.Framework.Services.LocalData) local PetsModule=require(ReplicatedStorage.Shared.Data.Pets)

if LocalData.IsReady and not LocalData:IsReady() and LocalData.DataReady then LocalData.DataReady:Wait() end

local function gd()local ok,d=pcall(function()return LocalData:Get()end)return ok and d or{}end local function fmtNum(n)n=tonumber(n)or 0;if n>=1e12 then return string.format("%.2fT",n/1e12)elseif n>=1e9 then return string.format("%.2fB",n/1e9)elseif n>=1e6 then return string.format("%.2fM",n/1e6)elseif n>=1e3 then return string.format("%.2fK",n/1e3)else return tostring(n)end end

local function fmtTime(sec)sec=math.max(0,math.floor(sec))local h=math.floor(sec/3600)local m=math.floor((sec%3600)/60)local s=sec%60 return string.format("%02d:%02d:%02d",h,m,s)end

local GUI=Instance.new("ScreenGui",lp.PlayerGui)GUI.IgnoreGuiInset=true GUI.ResetOnSpawn=false GUI.DisplayOrder=999999

local bg=Instance.new("Frame",GUI)bg.Size=UDim2.new(1,0,1,0)bg.BackgroundColor3=Color3.fromRGB(0,0,0)

local function Sec(x,y,w,h)local f=Instance.new("Frame",bg)f.AnchorPoint=Vector2.new(0.5,0.5)f.Position=UDim2.new(x,0,y,0)f.Size=UDim2.new(w,0,h,0)f.BackgroundTransparency=1 local ar=Instance.new("UIAspectRatioConstraint",f)ar.AspectRatio=4 ar.DominantAxis=Enum.DominantAxis.Height return f end local function Txt(p)local t=Instance.new("TextLabel",p)t.Size=UDim2.new(1,0,1,0)t.BackgroundTransparency=1 t.Font=Enum.Font.GothamBlack t.TextScaled=true t.TextColor3=Color3.fromRGB(255,255,255) t.TextWrapped=true return t end

local sessionFrame=Sec(0.5,0.12,0.6,0.12)local SESSION=Txt(sessionFrame)SESSION.Text="Session: 00:00:00"

local pepFrame=Sec(0.5,0.20,0.6,0.10)local PEP=Txt(pepFrame)PEP.Text="Peppermint Chest: 00:00"

local potFrame=Sec(0.22,0.08,0.45,0.10)local POT=Txt(potFrame)POT.TextXAlignment=Enum.TextXAlignment.Left POT.Text="Inventory Potions"

local snowBig=Sec(0.5,0.32,0.7,0.18)local BIG=Txt(snowBig) local rateFrame=Sec(0.5,0.44,0.5,0.10)local RATE=Txt(rateFrame)

local midRight=Sec(0.88,0.30,0.42,0.09)local MR=Txt(midRight)MR.TextXAlignment=Enum.TextXAlignment.Right

local petsFrame=Instance.new("Frame",bg)petsFrame.AnchorPoint=Vector2.new(0,1)petsFrame.Position=UDim2.new(0.02,0,0.86,0)petsFrame.Size=UDim2.new(0.38,0,0.22,0)petsFrame.BackgroundColor3=Color3.fromRGB(18,18,20)Instance.new("UICorner",petsFrame)

local petsTitle=Instance.new("TextLabel",petsFrame)petsTitle.Size=UDim2.new(1,-16,0,22)petsTitle.Position=UDim2.new(0,8,0,6)petsTitle.BackgroundTransparency=1 petsTitle.Font=Enum.Font.GothamBold petsTitle.TextSize=16 petsTitle.TextColor3=Color3.fromRGB(255,255,255) petsTitle.Text="ðŸ¾ Equipped Pets"

local petsList=Instance.new("TextLabel",petsFrame)petsList.Size=UDim2.new(1,-16,1,-34)petsList.Position=UDim2.new(0,8,0,30)petsList.BackgroundTransparency=1 petsList.Font=Enum.Font.Gotham petsList.TextSize=16 petsList.TextColor3=Color3.fromRGB(220,220,220) petsList.TextWrapped=true petsList.TextXAlignment=Enum.TextXAlignment.Left

local multiFrame=Instance.new("Frame",bg)multiFrame.AnchorPoint=Vector2.new(1,1)multiFrame.Position=UDim2.new(0.98,0,0.86,0)multiFrame.Size=UDim2.new(0.22,0,0.22,0)multiFrame.BackgroundColor3=Color3.fromRGB(18,18,20)Instance.new("UICorner",multiFrame) local multiTitle=Instance.new("TextLabel",multiFrame)multiTitle.Size=UDim2.new(1,-16,0,24)multiTitle.Position=UDim2.new(0,8,0,6)multiTitle.BackgroundTransparency=1 multiTitle.Font=Enum.Font.GothamBold multiTitle.TextSize=14 multiTitle.TextColor3=Color3.fromRGB(255,255,255) multiTitle.Text="Pet Enchants" local multiList=Instance.new("TextLabel",multiFrame)multiList.Size=UDim2.new(1,-16,1,-36)multiList.Position=UDim2.new(0,8,0,30)multiList.BackgroundTransparency=1 multiList.Font=Enum.Font.GothamBlack multiList.TextSize=18 multiList.TextColor3=Color3.fromRGB(220,220,220) multiList.TextWrapped=true multiList.TextXAlignment=Enum.TextXAlignment.Left

local msgFrame=Sec(0.52,0.50,0.75,0.11)local MSG=Txt(msgFrame)MSG.TextTransparency=1

local samples={}local shown=0 local function push(v)samples[#samples+1]={t=tick(),v=v}while #samples>1 and tick()-samples[1].t>60 do table.remove(samples,1)end end local function perMin()if #samples<2 then return 0 end local a=samples[1]local b=samples[#samples]if b.t-a.t<=0 or b.v-a.v<0 then return 0 end return((b.v-a.v)/(b.t-a.t))*60 end

local function getSnow()return gd().Snowflakes or 0 end local function getItems()local d=gd()local pots={FI=0,E4=0,E3=0}local p=d.Potions or{}for _,pt in pairs(p)do if type(pt)=="table"and pt.Name then if pt.Name=="Infinity Elixir"then pots.FI=pt.Amount or 0 end if pt.Name=="Festive Elixir IV"then pots.E4=pt.Amount or 0 end if pt.Name=="Festive Elixir III"then pots.E3=pt.Amount or 0 end end end return pots end

local function getPets()local d=gd()local eq={}local ids=(d.Teams and d.TeamEquipped and d.Teams[d.TeamEquipped]and d.Teams[d.TeamEquipped].Pets)or{}local map={}if d.Pets then for _,p in pairs(d.Pets)do map[tostring(p.Id)]=p end end for _,id in ipairs(ids)do local p=map[tostring(id)]if p then table.insert(eq,p)end end return eq end

local function chestLeft()local d=gd()local cds=d.Cooldowns if type(cds)="number"then return 0 end local now=os.time()local left=ts-now return left end

local function getEnchant(p)return (p.Enchant and p.Enchant.Name) or "None"end

local function teamCheck(n)return n:lower():match("team%-?up")~=nil end

local function pickRandom(list,n)local r={}local used={}for i=1,n do if #list==0 then break end local x repeat x=math.random(1,#list)until not used[x]used[x]=true table.insert(r,list[x])end return r end

local queue={}local lastEnchant={}local function addMsg(m)queue[#queue+1]=m end

local start=tick()

spawn(function() while true do local snow=getSnow()push(snow) local pots=getItems() POT.Text="Inventory Potions Festive Infinity Elixirs : "..pots.FI.." Festive Elixir IV : "..pots.E4.." Festive Elixir III : "..pots.E3

local pets=getPets()
    local lines={"Total Multi :"}

    local sum=0
    for _,p in ipairs(pets)do local st=PetsModule[p.Name]and PetsModule[p.Name].Stats and (PetsModule[p.Name].Stats.Snowflakes or 0) or 0 sum+=st end
    lines[1]="Total Multi : x"..fmtNum(sum).." â„ï¸"

    for _,p in ipairs(pets)do table.insert(lines,p.Name.." x"..fmtNum((PetsModule[p.Name]and PetsModule[p.Name].Stats and PetsModule[p.Name].Stats.Snowflakes)or 0).." â„ï¸")end

    petsList.Text=table.concat(lines,"

")

SESSION.Text="Session: "..fmtTime(tick()-start)

    local left=chestLeft()local m=math.floor(left/60)local s=left%60 local ss=string.format("%02d:%02d",m,s)
    PEP.Text="Peppermint Chest:

"..ss

BIG.Text="â„ï¸ "..fmtNum(snow)
    RATE.Text="â„ï¸/min: "..fmtNum(perMin())

    for _,p in ipairs(pets)do local en=getEnchant(p)if lastEnchant[p.Id]~=en then lastEnchant[p.Id]=en addMsg("Enchanting Pets")end end

    local chosen=pickRandom(pets,4)local elines={}
    for _,p in ipairs(chosen)do local en=getEnchant(p)local good=teamCheck(en) and "âœ…ï¸" or "âŒ" table.insert(elines,p.Name.." > "..en.." "..good)end
    multiList.Text=table.concat(elines,"

")

if #queue>0 then local m=table.remove(queue,1)MSG.Text=m MSG.TextTransparency=1 TweenService:Create(MSG,TweenInfo.new(0.5),{TextTransparency=0}):Play() wait(2) TweenService:Create(MSG,TweenInfo.new(0.5),{TextTransparency=1}):Play()end

    wait(1)
end

end)
