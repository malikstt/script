local samples = {}
local shown = 0
local function push(v)
	samples[#samples+1] = {t=tick(),v=v}
	while #samples>1 and tick()-samples[1].t>60 do
		table.remove(samples,1)
	end
end

local function perMin()
	if #samples<2 then return 0 end
	local a,b = samples[1],samples[#samples]
	if b.t-a.t<=0 or b.v-a.v<0 then return 0 end
	return ((b.v-a.v)/(b.t-a.t))*60
end

local queue = {}
local function addMsg(m)
	if queue[#queue] ~= m then
		queue[#queue+1] = m
	end
end

local function fadeMsg(t)
	MSG.Text = t
	MSG.TextTransparency = 1
	TweenService:Create(MSG,TweenInfo.new(0.4),{TextTransparency=0}):Play()
end

local startTick = tick()
local last = {snow=getSnow(),hatches=getHatches()}

while true do
	local snow = getSnow()
	push(snow)

	BIG.Text = "â„ï¸ "..fmt_num(snow)
	SESSION.Text = "â±ï¸ Session: "..fmt_hms(tick()-startTick)
	PEP.Text = "ðŸ¬ Peppermint Chest:\n"..fmt_mmss_or_ready(getPepRemainingSeconds())

	local items = getItems()
	local pres = items["Christmas Present"] or 0
	local keys = items["Christmas Key"] or 0
	local spins = items["Christmas Spin Ticket"] or 0

	local inf, elix = getPotionsTwoLines()
	TL.Text = "ðŸ§ª Inventory potions\nâœ¨ Festive Infinity Elixirs : "..fmt_num(inf).."\nðŸ”® Festive Elixirs : "..fmt_num(elix)
	ML.Text = "ðŸŽ Christmas Presents: "..fmt_num(pres)
	MR.Text = "ðŸ—ï¸ "..fmt_num(keys).."\nðŸŽ¡ "..fmt_num(spins)

	local arr,sum = getTeam()
	if #arr>0 then
		local lines = {}
		lines[#lines+1] = "â„ï¸ Total Multi : "..fmt_num(sum)
		lines[#lines+1] = ""
		for _,p in ipairs(arr) do
			lines[#lines+1] = "ðŸ¾ "..p.Name.." x"..fmt_num(p.Snow).." â„ï¸"
		end
		petsList.Text = table.concat(lines,"\n")

		local el = {}
		for _,p in ipairs(arr) do
			local mark = hasTeamUpEnchant(p) and "âœ…" or "âŒ"
			el[#el+1] = "ðŸ¾ "..p.Name.." > "..firstEnchantId(p).." "..mark
		end
		enchantList.Text = table.concat(el,"\n")
	else
		petsList.Text = "None"
		enchantList.Text = "None"
	end

	if snow>last.snow then addMsg("â„ï¸ Automatically Giving Giftsâ€¦") end
	if getHatches()>last.hatches then addMsg("ðŸ¥š Hatching Eggs!") end
	last.snow = snow
	last.hatches = getHatches()

	if #queue>0 then
		local m = table.remove(queue,1)
		fadeMsg(m)
		task.wait(math.random(6,12))
		TweenService:Create(MSG,TweenInfo.new(0.35),{TextTransparency=1}):Play()
	end

	task.wait(1)
end
