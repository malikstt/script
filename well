_G.Config = { UserID = "3087df52-2f5c-4e6f-a407-10fa72d16932", discord_id = "1135837863603351573" , Note = "Phone ", } loadstring(game:HttpGet("https://raw.githubusercontent.com/skadidau/unfazedfree/refs/heads/main/bgsi"))()

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")

local RemoteEvent = ReplicatedStorage:WaitForChild("Shared"):WaitForChild("Framework"):WaitForChild("Network"):WaitForChild("Remote"):WaitForChild("RemoteEvent")

local oldJobId = game.JobId
local startTime = os.time()

local function equipBestPets()
    RemoteEvent:FireServer("EquipBestPets")
end

local function teleportToZen()
    RemoteEvent:FireServer("Teleport", "Workspace.Worlds.The Overworld.Islands.Zen.Island.Portal.Spawn")
end

local function useMysteryBox()
    RemoteEvent:FireServer("UseGift", "Mystery Box", 25)
end

local function claimGift(giftID)
    RemoteEvent:FireServer("ClaimGift", giftID)
end

while true do
    equipBestPets()
    teleportToZen()
    
    useMysteryBox()
    task.wait(0.5)
    
    local rendered = workspace:FindFirstChild("Rendered")
    if rendered then
        local giftsFolder = rendered:FindFirstChild("Gifts")
        if giftsFolder then
            for _, gift in pairs(giftsFolder:GetChildren()) do
                claimGift(gift.Name)
            end
        end
    end
    task.wait(0.5)
    
    if os.time() - startTime >= 5400 then
        TeleportService:Teleport(game.PlaceId, Players.LocalPlayer)
        break
    end
    
    task.wait(300)
end

task.wait(5)

if game.JobId ~= oldJobId then
    getgenv().RIFT_EGG = "Lunar Egg"
    getgenv().KEEP_RARITIES = {"Secret", "Infinity"}
    getgenv().AUTO_SUMMON = true
    getgenv().AUTO_HATCH = true
    getgenv().AUTO_DELETE = true
    getgenv().REMOVE_EGG_ANIMATION = true
    getgenv().WORLD_2_SUMMON = false
    getgenv().ALWAYS_EVOLVED_POTIONS = true
    getgenv().ALWAYS_EGG_ELIXIR = true
    getgenv().ALWAYS_INFINITY_ELIXIR = true
    getgenv().ALWAYS_SECRET_ELIXIR = false

    loadstring(game:HttpGet("https://raw.githubusercontent.com/malikstt/script/main/riftfinder"))()
end
