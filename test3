local ReplicatedStorage = game:GetService("ReplicatedStorage")
local VirtualInputManager = game:GetService("VirtualInputManager")

local RemoteEvent = ReplicatedStorage.Shared.Framework.Network.Remote.RemoteEvent

RemoteEvent:FireServer("WorldTeleport", "Christmas World")

task.wait(5)

task.spawn(function()
    while true do
        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.B, false, game)
        task.wait(0.1)
        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.B, false, game)
        task.wait(60)
    end
end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/IdiotHub/Scripts/refs/heads/main/BGSI/main.lua"))()
