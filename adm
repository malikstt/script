setfpscap(3)
script_key="workfine";
getgenv().Config = {
    ["PetAMountToBuy"] = "1", 
    ["PetRemoteToBuy"] = "aztec_egg_2025_aztec_egg",
    ["PetFarmAutoSwitchFullGrown"] = false,
    ["PetFarmActive"] = false,
    ["LitePetFarmActive"] = true,
    ["EggFarmActive"] = false,
    ["HideUselessGui"] = true,
    ["Blur_username"] = false,
    ["Blazing_Lion_Log"] = false,
    ["DiscordId"] = "1135837863603351573",
    ["Webhook"] = "https://discord.com/api/webhooks/1400469457427632220/Ii2SL9L5Ixbh7czjLXiIKZMthn3nTNWXR7w0mFmcYgnutB6XTYUaZjZAoF0DgPh05Pna"
}

loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/66567bfd337b57eb059b58dbe1badb89.lua"))()

if settings and settings().Rendering then
    settings().Rendering.QualityLevel = 1
    settings().Rendering.MeshCacheSize = 0
    settings().Rendering.EnableFRM = false
end

sethiddenproperty(game:GetService("Lighting"), "Technology", 2)
sethiddenproperty(game:GetService("Lighting"), "GlobalShadows", false)
local lighting = game:GetService("Lighting")
lighting.Brightness = 1
lighting.ClockTime = 14
lighting.GeographicLatitude = 0
lighting.Outlines = false
lighting.FogEnd = 100000

game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.All, false)

for _, v in pairs(game:GetDescendants()) do
    if v:IsA("Part") or v:IsA("MeshPart") or v:IsA("UnionOperation") then
        v.Material = Enum.Material.Plastic
        if v:FindFirstChildOfClass("Texture") then
            v:FindFirstChildOfClass("Texture"):Destroy()
        end
    elseif v:IsA("Decal") or v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Beam") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") or v:IsA("SurfaceGui") or v:IsA("BillboardGui") or v:IsA("ShirtGraphic") then
        v:Destroy()
    elseif v:IsA("SpecialMesh") then
        v.TextureId = ""
    end
end
