repeat task.wait() until game:IsLoaded()

local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local TeleportService = game:GetService("TeleportService")

local API_BASE = "https://chips-workers-hammer-consulting.trycloudflare.com"
local CHECK_INTERVAL = 3

local LP = Players.LocalPlayer
local lastJoinedJobId = nil

local function getAlts()
	local res = request({
		Url = API_BASE .. "/alts",
		Method = "GET"
	})

	if not res or not res.Body then
		return nil
	end

	local decoded
	pcall(function()
		decoded = HttpService:JSONDecode(res.Body)
	end)

	return decoded
end

task.spawn(function()
	while true do
		local data = getAlts()

		if data and data.alts then
			for _, alt in ipairs(data.alts) do
				local jobId = alt.jobId
				local spotsLeft = tonumber(alt.spotsLeft) or 0
				local mainPresent = alt.mainPresent
				local hasSecret = alt.hasSecret

				if spotsLeft > 0
					and mainPresent == false
					and hasSecret == true
					and jobId ~= lastJoinedJobId
				then
					lastJoinedJobId = jobId

					print("[MAIN] Joining job:", jobId)
					TeleportService:TeleportToPlaceInstance(
						game.PlaceId,
						jobId,
						LP
					)
					break
				end
			end
		end

		task.wait(CHECK_INTERVAL)
	end
end)

