local VirtualInputManager = game:GetService("VirtualInputManager")
local screenSize = workspace.CurrentCamera.ViewportSize
local touchPosition = Vector2.new(screenSize.X / 2, screenSize.Y / 2)

while true do
    VirtualInputManager:SendMouseButtonEvent(touchPosition.X, touchPosition.Y, 0, true, game, 1)
    task.wait()
    VirtualInputManager:SendMouseButtonEvent(touchPosition.X, touchPosition.Y, 0, false, game, 1)
    task.wait(60)
end
local args = {
    "WorldTeleport",
    "Minigame Paradise"
}
game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Framework"):WaitForChild("Network"):WaitForChild("Remote"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
