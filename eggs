local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RemoteEvent = ReplicatedStorage:WaitForChild("Shared"):WaitForChild("Framework"):WaitForChild("Network"):WaitForChild("Remote"):WaitForChild("RemoteEvent")

local args1 = {"HatchPowerupEgg", "Duality Egg", 12}
RemoteEvent:FireServer(unpack(args1))

wait(2)

local args2 = {"ToggleAutoDelete", "Disturbed Unicorn"}
RemoteEvent:FireServer(unpack(args2))

wait(1)

for i = 1, 5 do
    RemoteEvent:FireServer(unpack(args1))
    wait(1)
end

local args3 = {"ToggleAutoDelete", "Disturbed Dragon"}
RemoteEvent:FireServer(unpack(args3))

wait(1)

while true do
    RemoteEvent:FireServer(unpack(args1))
    wait(1)
end
