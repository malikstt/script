repeat task.wait() until game:IsLoaded()
local lp = game.Players.LocalPlayer
repeat task.wait() until lp:FindFirstChild("PlayerGui")
task.wait(math.random(0,3000)/1000)
local g = lp.PlayerGui:FindFirstChild("ScreenGui")
if g and g:FindFirstChild("DailyRewards") then
    g.DailyRewards:Destroy()
end
FOCUS_DICE = true
ALWAYS_DICE_CHEST = true
USE_GOLDEN_DICE = true
GOLDEN_DICE_DISTANCE = 3
DICE_DISTANCE = 9
GIANT_DICE_DISTANCE = 50
loadstring(game:HttpGet("https://raw.githubusercontent.com/malikstt/script/main/w1"))()
