_G.Config={UserID="3087df52-2f5c-4e6f-a407-10fa72d16932",discord_id="1135837863603351573",Note="Pc"}
local s
for i=1,5 do
    s = pcall(function()
        loadstring(game:HttpGet("https://cdn.yummydata.click/scripts/bgsi"))()
    end)
    if s then break end
    wait(5)
end

task.wait(15)
setfpscap(5)
FOCUS_DICE = true
ALWAYS_DICE_CHEST = true
USE_GOLDEN_DICE = true
GOLDEN_DICE_DISTANCE = 2
DICE_DISTANCE = 9
GIANT_DICE_DISTANCE = 50
loadstring(game:HttpGet("https://raw.githubusercontent.com/malikstt/script/main/w1"))()

loadstring(game:HttpGet("https://raw.githubusercontent.com/malikstt/script/refs/heads/main/optimization"))()

task.spawn(function()
    local function sendWebhook(url, data)
        local req = http_request or request or (syn and syn.request) or (fluxus and fluxus.request)
        req({
            Url = url,
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = game:GetService("HttpService"):JSONEncode(data)
        })
    end

    local Players = game:GetService("Players")
    local player = Players.LocalPlayer
    local playerGui = player:WaitForChild("PlayerGui")

    local function isText(o)
        return o:IsA("TextLabel") or o:IsA("TextButton") or o:IsA("TextBox")
    end

    local BOOSTS = {
        "Luck",
        "Speed",
        "Mythic",
        "Infinity Elixir",
        "Egg Elixir",
        "Coins",
    }

    local function waitForBuffs()
        local buffs
        repeat
            buffs = playerGui:FindFirstChild("Buffs", true)
            if not buffs then task.wait(0.1) end
        until buffs
        return buffs
    end

    local function scanBoosts()
        local buffs = waitForBuffs()
        local lines = {}

        for _, name in ipairs(BOOSTS) do
            local folder = buffs:FindFirstChild(name)
            if folder then
                local btn = folder:FindFirstChild("Button")
                if btn then
                    local label = btn:FindFirstChild("Label")
                    if label and isText(label) then
                        local t = label.Text
                        if string.find(t, "days") or string.find(t, "hours") then
                            table.insert(lines, name .. " â†’ " .. t)
                        end
                    end
                end
            end
        end

        return lines
    end

    local function scanPotions()
        local potionsFolder = playerGui:FindFirstChild("Potions", true)
        if not potionsFolder then return {} end

        local items = potionsFolder:FindFirstChild("Items")
        if not items then return {} end

        local out = {}

        for _, potion in ipairs(items:GetChildren()) do
            local inner = potion:FindFirstChild("Inner")
            if inner then
                local btn = inner:FindFirstChild("Button")
                if btn then
                    local bin = btn:FindFirstChild("Inner")
                    if bin then
                        local title = bin:FindFirstChild("Title")
                        local tier = bin:FindFirstChild("Tier")
                        local amount = bin:FindFirstChild("Amount")

                        if isText(title) and isText(tier) and isText(amount) then
                            local t = tier.Text
                            if t == "Tier 6" or t == "Tier 7" then
                                table.insert(out, title.Text .. " | " .. t .. " | " .. amount.Text)
                            end
                        end
                    end
                end
            end
        end

        return out
    end

    while true do
        local boosts = scanBoosts()
        local potions = scanPotions()

        local msg =
            "**" .. player.Name .. "**\n\n" ..
            table.concat(boosts, "\n") ..
            "\n\n" ..
            table.concat(potions, "\n")

        sendWebhook(
            "https://discord.com/api/webhooks/1438235226261426277/epAuKNwoDb5lJ_A9bgdoDKKtZVRJbEuYVWGeyoBMSDoooWaDifn1AoeKnLohP5B6awii",
            {content = msg}
        )

        task.wait(math.random(3600, 7200))
    end
end)
