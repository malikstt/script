repeat task.wait() until game:IsLoaded()
local lp = game.Players.LocalPlayer
repeat task.wait() until lp:FindFirstChild("PlayerGui")
task.wait(math.random(0,3000)/1000)

local g = lp.PlayerGui:FindFirstChild("ScreenGui")
if g and g:FindFirstChild("DailyRewards") then
    g.DailyRewards:Destroy()
end
getgenv().Settings = {
  Trading = {
    Enabled = true,

    MainAccountNames = {"MALIKSTOM"},
    AltAccountNames = {"Ebikeu"},

    OnlyTradeLimitedSecrets = false,
    MainAccountRejoin = true,
    AltAccountRejoin = true,
    RejoinDelaySeconds = 0,

    Webhook = {
      Enabled = false,
      AltWebhookUrl = "",
      MainWebhookUrl = ""
    }
  }
}

loadstring(game:HttpGet("https://raw.githubusercontent.com/issascripts/bgsi/refs/heads/main/myfriggincoolautotradescript.lua"))()
