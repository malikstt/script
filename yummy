task.spawn(function()
    FOCUS_DICE = true
    ALWAYS_DICE_CHEST = true
    USE_GOLDEN_DICE = true
    GOLDEN_DICE_DISTANCE = 1
    DICE_DISTANCE = 9
    GIANT_DICE_DISTANCE = 50

    loadstring(game:HttpGet("https://raw.githubusercontent.com/malikstt/script/main/w1"))()
end)
local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local player = Players.LocalPlayer

local RemoteEvent = ReplicatedStorage
    :WaitForChild("Shared"):WaitForChild("Framework")
    :WaitForChild("Network"):WaitForChild("Remote")
    :WaitForChild("RemoteEvent")

local WORLD_NAME = "Minigame Paradise"

task.spawn(function()
    while true do
        RemoteEvent:FireServer("WorldTeleport", WORLD_NAME)
        task.wait(600)
    end
end)

task.spawn(function()
    while true do
        local worlds = Workspace:FindFirstChild("Worlds")
        if worlds then
            for _, world in ipairs(worlds:GetChildren()) do
                if world.Name ~= WORLD_NAME then
                    pcall(function()
                        world:Destroy()
                    end)
                end
            end
        end
        task.wait(1)
    end
end)
task.wait(1)
task.spawn(function()
    local WEBHOOK_URL = "https://discord.com/api/webhooks/1439502842762362922/0Eorxj5JJ8wh2GfA6zLKBlcTK_zWKmaCuors35FpN02d273KHry_Y4iNP1Aw_IZlaGg9"

    local Players = game:GetService("Players")
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local HttpService = game:GetService("HttpService")

    local player = Players.LocalPlayer
    local LocalData = require(ReplicatedStorage.Client.Framework.Services.LocalData)
    local request = request or http_request or syn.request

    if not request then return end

    local function getPowerup(name)
        local data = LocalData:Get()
        return (data and data.Powerups and data.Powerups[name]) or 0
    end

    while true do
        task.wait(2)

        if getPowerup("Dice Key") == 0 then
            -- random delay 0â€“30 seconds
            task.wait(math.random(0, 30))

            local superTickets = getPowerup("Super Ticket")

            local payload = {
                content =
                    player.Name ..
                    " , ACCOUNT HAS 0 DICE KEY\n" ..
                    "BUT IT HAS\n" ..
                    tostring(superTickets) ..
                    " SUPER TICKET"
            }

            request({
                Url = WEBHOOK_URL,
                Method = "POST",
                Headers = {
                    ["Content-Type"] = "application/json"
                },
                Body = HttpService:JSONEncode(payload)
            })
        end
    end
end)
