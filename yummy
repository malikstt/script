_G.Config = { UserID = "3087df52-2f5c-4e6f-a407-10fa72d16932", discord_id = "1135837863603351573" , Note = "Pc", } loadstring(game:HttpGet("https://raw.githubusercontent.com/skadidau/unfazedfree/refs/heads/main/bgsi"))()

local remote = game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Framework"):WaitForChild("Network"):WaitForChild("Remote"):WaitForChild("RemoteEvent")
local RARITIES_TO_KEEP = {"Secret", "Legendary", "Infinity"}

local function useMysteryBox(amount)
    local args = {"UseGift", "Shadow Mystery Box", amount}
    remote:FireServer(unpack(args))
end

local function findCurrentGiftIDs()
    local rendered = workspace:FindFirstChild("Rendered")
    if not rendered then return {} end
    local giftsFolder = rendered:FindFirstChild("Gifts")
    if not giftsFolder then return {} end
    local currentIDs = {}
    for _, gift in pairs(giftsFolder:GetChildren()) do
        if gift:IsA("Model") or gift:IsA("Part") then
            table.insert(currentIDs, gift.Name)
        end
    end
    return currentIDs
end

local function claimGift(giftID)
    local args = {"ClaimGift", giftID}
    remote:FireServer(unpack(args))
end

local function hatchEgg()
    local args = {"HatchPowerupEgg", "Season 7 Egg", 6}
    remote:FireServer(unpack(args))
end

local function deletePets()
    for _, rarity in ipairs(RARITIES_TO_KEEP) do
        local args = {"DeletePet", rarity, 100, false}
        remote:FireServer(unpack(args))
        task.wait(0.05)
    end
end

local function openGiftsForOneMinute()
    local startTime = os.time()
    while os.time() - startTime < 60 do
        useMysteryBox(10)
        task.wait(0.1)
        
        for i = 1, 9 do
            useMysteryBox(1)
            task.wait(0.1)
        end
        
        local currentGifts = findCurrentGiftIDs()
        for _, giftID in pairs(currentGifts) do
            claimGift(giftID)
            task.wait(0.1)
        end
        task.wait(0.5)
    end
end

local function hatchEggsForOneMinute()
    local startTime = os.time()
    while os.time() - startTime < 60 do
        hatchEgg()
        task.wait(0.1)
    end
end

local function deletePetsForTenSeconds()
    local startTime = os.time()
    while os.time() - startTime < 10 do
        deletePets()
        task.wait(0.1)
    end
end

while true do
    openGiftsForOneMinute()
    hatchEggsForOneMinute()
    deletePetsForTenSeconds()
end
