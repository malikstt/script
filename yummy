_G.Config={UserID="3087df52-2f5c-4e6f-a407-10fa72d16932",discord_id="1135837863603351573",Note="Pc"}

for i=1,5 do if pcall(function() loadstring(game:HttpGet("https://cdn.yummydata.click/scripts/bgsi"))() end) then break end task.wait(1) end

FOCUS_DICE=true
ALWAYS_DICE_CHEST=true
USE_GOLDEN_DICE=true
GOLDEN_DICE_DISTANCE=2
DICE_DISTANCE=9
GIANT_DICE_DISTANCE=50
loadstring(game:HttpGet("https://raw.githubusercontent.com/malikstt/script/main/w2"))()

local P=game:GetService("Players") local RS=game:GetService("ReplicatedStorage") local WS=game:GetService("Workspace")
local LP=P.LocalPlayer local LD=require(RS.Client.Framework.Services.LocalData)
local RE=RS.Shared.Framework.Network.Remote.RemoteEvent local OF=WS.Obbys
local OT=WS.Worlds["Seven Seas"].Areas["Classic Island"].Obbys

local G=Instance.new("ScreenGui",LP:WaitForChild("PlayerGui")) G.ResetOnSpawn=false
local B=Instance.new("TextButton",G) B.Size=UDim2.new(0,180,0,50) B.Position=UDim2.new(.5,-90,.9,-25)
B.TextScaled=true B.Font=Enum.Font.GothamBold B.BackgroundColor3=Color3.fromRGB(255,0,0)
B.TextColor3=Color3.fromRGB(255,255,255) B.Text="AUTO OBBY: OFF"

local ON=false local D={"Easy","Medium","Hard"}

local function tp(t)local c=LP.Character if not c or not t then return end
local cf=typeof(t)=="CFrame" and t or t:IsA("BasePart") and t.CFrame or t:IsA("Model") and t:GetPivot()
if cf then c:PivotTo(cf*CFrame.new(0,3,0)) end end

local function cycle(d)local p=OT:FindFirstChild(d) and OT[d]:FindFirstChild("Portal") and OT[d].Portal:FindFirstChild("Part")
local c=OF:FindFirstChild(d) and OF[d]:FindFirstChild("Complete") if not p or not c then return end
tp(p) task.wait(.1) RE:FireServer("StartObby",d) task.wait(.25) tp(c) task.wait(.1)
RE:FireServer("CompleteObby") task.wait(.1) for i=1,50 do if not ON then break end RE:FireServer("ClaimObbyChest") task.wait(.1) end end

task.spawn(function()
while task.wait(.2) do if not ON then continue end
local c=LP.Character local pd=LD:Get() if not c or not c.PrimaryPart or not pd or not pd.ObbyCooldowns then continue end
local s=c.PrimaryPart.CFrame for _,d in ipairs(D) do if not ON then break end
local cd=pd.ObbyCooldowns[d] or 0 if os.time()>=cd then cycle(d) pd=LD:Get() end end tp(s)
local nt=math.huge for _,d in ipairs(D) do local cd=pd.ObbyCooldowns[d] or 0 if cd>os.time() and cd<nt then nt=cd end end
local w=nt-os.time() if w>0 then task.wait(w) end end end)

B.MouseButton1Click:Connect(function() ON=not ON
B.BackgroundColor3=ON and Color3.fromRGB(0,255,0) or Color3.fromRGB(255,0,0)
B.Text=ON and "AUTO OBBY: ON" or "AUTO OBBY: OFF" end)
