loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/e1f274825da00b92599872de596f1fc0.lua"))()

local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local url = "https://discord.com/api/webhooks/1362479671387619499/YXQdRgCPSjHR0IrMUbb0UeULweAosEQaWGLtxMp80hexjlPtk0jD0q5mNwkdudOZpb7C"

local function sendWebhook(data)
    local success, response = pcall(function()
        http_request({
            Url = url,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = HttpService:JSONEncode(data)
        })
    end)
end

local function GetPotionTime(potionName)
    local player = Players.LocalPlayer
    if not player then return "N/A" end
    
    local playerGui = player:FindFirstChild("PlayerGui")
    if not playerGui then return "N/A" end
    
    local screenGui = playerGui:FindFirstChild("ScreenGui")
    if not screenGui then return "N/A" end
    
    local buffs = screenGui:FindFirstChild("Buffs")
    if not buffs then return "N/A" end
    
    local potion = buffs:FindFirstChild(potionName)
    if not potion then return "N/A" end
    
    local button = potion:FindFirstChild("Button")
    if not button then return "N/A" end
    
    local label = button:FindFirstChild("Label")
    if not label then return "N/A" end
    
    return label.Text
end

local function GetPlayTime()
    local player = Players.LocalPlayer
    if not player then return "N/A" end
    
    local playerGui = player:FindFirstChild("PlayerGui")
    if not playerGui then return "N/A" end
    
    local screenGui = playerGui:FindFirstChild("ScreenGui")
    if not screenGui then return "N/A" end
    
    local debugTags = screenGui:FindFirstChild("_debug")
    if debugTags then
        local tags = debugTags:FindFirstChild("Tags")
        if tags then
            local template = tags:FindFirstChild("Template")
            if template then
                local label = template:FindFirstChild("Label")
                if label and label:IsA("TextLabel") then
                    return label.Text:gsub("<[^>]+>", "")
                end
            end
        end
    end
    
    return "N/A"
end

local function sendPotionWebhook()
    local player = Players.LocalPlayer
    local username = player and player.Name or "N/A"
    local playtime = GetPlayTime()

    local potions = {
        "Lucky",
        "Speed", 
        "Infinity Elixir",
        "Mythic",
        "Tickets",
        "Coins",
        "Secret Elixir",
        "Egg Elixir"
    }

    local message = "```\n"
    message = message .. "Player: " .. username .. "\n"
    message = message .. "Playtime: " .. playtime .. "\n\n"
    message = message .. "POTIONS:\n"

    for _, potionName in ipairs(potions) do
        local time = GetPotionTime(potionName)
        message = message .. potionName .. ": " .. time .. "\n"
    end

    message = message .. "```"

    sendWebhook({content = message})
end

local function autoUsePotions()
    getgenv().running = true
    
    local potions = {
        {"Coins", 6}, {"Coins", 5},
        {"Speed", 6},
        {"Lucky", 6},
        {"Mythic", 6},
        {"Tickets", 6}, {"Tickets", 5},
        {"Infinity Elixir", 1}
    }

    while getgenv().running do
        for _, potion in ipairs(potions) do
            local args = {"UsePotion", potion[1], potion[2]}
            game:GetService("ReplicatedStorage").Shared.Framework.Network.Remote.RemoteEvent:FireServer(unpack(args))
            task.wait(0.5)
        end
        task.wait(300)
    end
end

spawn(autoUsePotions)

sendPotionWebhook()
while true do
    task.wait(300)
    sendPotionWebhook()
end
