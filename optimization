FOCUS_DICE = true
ALWAYS_DICE_CHEST = false
USE_GOLDEN_DICE = true
GOLDEN_DICE_DISTANCE = 3
DICE_DISTANCE = 9
GIANT_DICE_DISTANCE = 50

task.spawn(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/malikstt/script/main/w1"))()
end)

task.spawn(function()
    task.wait(math.random(1,3))
    loadstring(game:HttpGet("https://raw.githubusercontent.com/IdiotHub/Scripts/main/Loader"))()
end)
