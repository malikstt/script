_G.Config={UserID="3087df52-2f5c-4e6f-a407-10fa72d16932",discord_id="1135837863603351573",Note="Pc"}local s;for i=1,5 do s=pcall(function()loadstring(game:HttpGet("https://raw.githubusercontent.com/skadidau/unfazedfree/refs/heads/main/bgsi"))()end)if s then break end wait(5)end
task.spawn(function()
	local TeleportService = game:GetService("TeleportService")
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")

	local PLACE_ID = game.PlaceId

	local HARD_TIMEOUT = 180
	local HEARTBEAT_TIMEOUT = 60

	local start = os.clock()
	local lastHeartbeat = os.clock()

	RunService.Heartbeat:Connect(function()
		lastHeartbeat = os.clock()
	end)

	repeat task.wait() until game:IsLoaded()

	local player
	repeat
		task.wait(0.2)
		player = Players.LocalPlayer
	until player

	local function isPlayerLoaded()
		return player:FindFirstChild("PlayerGui")
			and player.Character
			and player.Character:FindFirstChild("HumanoidRootPart")
	end

	while not isPlayerLoaded() do
		task.wait(0.5)

		if os.clock() - lastHeartbeat > HEARTBEAT_TIMEOUT then
			TeleportService:Teleport(PLACE_ID)
			return
		end

		if os.clock() - start > HARD_TIMEOUT then
			TeleportService:Teleport(PLACE_ID)
			return
		end
	end
end)
