_G.Config={UserID="3087df52-2f5c-4e6f-a407-10fa72d16932",discord_id="1135837863603351573",Note="Pc"}local s;for i=1,5 do s=pcall(function()loadstring(game:HttpGet("https://raw.githubusercontent.com/skadidau/unfazedfree/refs/heads/main/bgsi"))()end)if s then break end wait(5)end
require(game:GetService("ReplicatedStorage").Client.Effects.HatchEgg).Play = function() return end
game:GetService("RunService"):Set3dRenderingEnabled(false)

getgenv().running = not getgenv().running

local RS = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

local datamodule = require(RS.Client.Framework.Services.LocalData)
local data = datamodule:Get()

local Remote = RS
    :WaitForChild("Shared")
    :WaitForChild("Framework")
    :WaitForChild("Network")
    :WaitForChild("Remote")
    :WaitForChild("RemoteEvent")

task.spawn(function()
    for Item, Amount in pairs(data.Powerups) do
        if Item:match("Egg") then
            if Amount > 12 then
                local Times = math.floor(Amount / 12)
                print(string.format("Opening %s x%d", Item, Times * 12))
                for i = 1, Times do
                    Remote:FireServer("HatchPowerupEgg", Item, 12)
                    task.wait(0.75)
                end
                task.wait(2)
            elseif Amount > 0 then
                print(string.format("Opening %s x%d", Item, Amount))
                Remote:FireServer("HatchPowerupEgg", Item, Amount)
            end
        end
    end
end)

task.spawn(function()
    local pots = {
        {"UsePotion", "Speed", 7, 10},
        {"UsePotion", "Lucky", 7, 10},
        {"UsePotion", "Mythic", 7, 10},
        {"UsePotion", "Egg Elixir", 1, 10},
        {"UsePotion", "Infinity Elixir", 1, 10},
        {"UsePotion", "Festive Elixir", 4, 10},
        {"UsePotion", "Festive Infinity Elixir", 1, 10},
        {"UsePotion", "Ultra Infinity Elixir", 1, 10},
    }

    local e = 60

    if not getgenv().running then
        RunService:UnbindFromRenderStep("PotionSpam")
        return
    end

    RunService:BindToRenderStep(
        "PotionSpam",
        Enum.RenderPriority.First.Value,
        function()
            if not getgenv().running then return end
            for i = 1, e do
                for _, args in ipairs(pots) do
                    Remote:FireServer(unpack(args))
                end
            end
        end
    )
end)
