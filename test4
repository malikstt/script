loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/a1b4af844e9ed9a7b2a2cc2ee90fe37d.lua"))()
task.spawn(function()
    local ReplicatedStorage = game:GetService("ReplicatedStorage")

    local RemoteEvent = ReplicatedStorage
        :WaitForChild("Shared")
        :WaitForChild("Framework")
        :WaitForChild("Network")
        :WaitForChild("Remote")
        :WaitForChild("RemoteEvent")

    local LocalData = require(
        ReplicatedStorage
            :WaitForChild("Client")
            :WaitForChild("Framework")
            :WaitForChild("Services")
            :WaitForChild("LocalData")
    )

    local PetsModule = require(
        ReplicatedStorage
            :WaitForChild("Shared")
            :WaitForChild("Data")
            :WaitForChild("Pets")
    )

    local running = false

    local function isProtectedPet(petName)
        local info = PetsModule[petName]
        if not info then return true end
        if info.Secret == true or info.IsSecret == true then return true end
        if info.Rarity and type(info.Rarity) == "string" then
            local r = info.Rarity:lower()
            if r == "secret" or r == "infinity" then return true end
        end
        if info.Tags and type(info.Tags) == "table" then
            for _, tag in ipairs(info.Tags) do
                if type(tag) == "string" then
                    local t = tag:lower()
                    if t == "secret" or t == "infinity" then return true end
                end
            end
        end
        return false
    end

    local function runAutoDelete()
        if running then return end
        running = true

        local data = LocalData:Get()
        if not data then running = false return end

        local pets = data.Pets or {}
        local autoDelete = data.AutoDelete or {}

        for _, pet in pairs(pets) do
            if pet and pet.Name then
                local name = pet.Name
                if not autoDelete[name] and not isProtectedPet(name) then
                    RemoteEvent:FireServer("ToggleAutoDelete", name)
                    task.wait(0.05)
                end
            end
        end

        running = false
    end

    runAutoDelete()
    task.delay(5, runAutoDelete)
    task.delay(15, runAutoDelete)
end)
